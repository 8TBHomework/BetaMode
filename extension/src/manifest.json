{
  "name": "Betamode",
  "version": "0.0.0",
  "description": "Betachip for your browser",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "betamode@8tbhomework.github.io"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "tabs"
  ]
}
